   "source": [
    "# Before we start, we need to add extra column for x_0\n",
    "# It will allow us to use matrix multiplication while \n",
    "# calculating our hypothesis\n",
    "ones = np.ones((X.shape[0], 1))\n",
    "\n",
    "X = np.append(ones, X, axis=1)\n",
