   ],
   "source": [
    "alpha = 0.01\n",
    "\n",
    "J_arr = []\n",
    "n = 0\n",
    "\n",
    "while J > 0.001 and n < 10000:\n",
    "    J_arr.append(J)\n",
    "\n",
    "    t = alpha * ((h_theta - Y).T.dot(X)) * 1/m \n",
    "    theta = theta - t.T\n",
    "\n",
    "    h_theta = X * theta\n",
    "    J = np.sum(np.power(h_theta - Y, 2)) * 1/(2*m)\n",
    "\n",
    "    n += 1\n",
    "\n",
    "theta    "
